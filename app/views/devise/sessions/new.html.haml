.logins
  .logins__header
    =link_to image_tag('logo.png', class: 'login__header__image'), "/informations"
    
  .logins__row
    .logins__row__container

      .logins__row__container__header
        %h2 連携済みアカウントでログイン

      .logins__row__container__icons
        .logins__row__container__icons__icon
          =link_to ""
        .logins__row__container__icons__icon
          = link_to image_tag('data:image/png;base64,',size: "75*75"),""
        .logins__row__container__icons__icon
          = link_to image_tag('https://is3-ssl.mzstatic.com/image/thumb/Purple114/v4/47/72/0c/47720c48-9bff-c435-1397-e716e12d6187/Icon-Production-0-0-1x_U007emarketing-0-0-0-7-0-0-sRGB-0-0-0-GLES2_U002c0-512MB-85-220-0-0.png/246x0w.png',size: "75*75"),""
        .logins__row__container__icons__icon
          =link_to ""
    
      .logins__row__container__border
        .logins__row__container__border__beforeborder
        %p もしくは
        .logins__row__container__border__afterborder
    
      .logins__row__container__middle
        %h2 メールアドレスでログイン

      .logins__row__container__form

        = form_for(resource, as: resource_name, url: session_path(resource_name)) do |f|
          .form-field
            .form-field__input
              = f.email_field :email, autofocus: true, placeholder: "メールアドレス"
            .form-field__input
              = f.password_field :password, autofocus: true, placeholder: "パスワード"
            .form-field__button
              = f.submit "ログイン", class: 'btn'
        
      .logins__row__container__restart
        = link_to "#" do
          パスワード、ログイン方法を忘れた方はこちらから

  .logins__row__br

  .logins__row__registation
    .logins__row__registation__text
      %h2 フリマに未登録の方は
      = link_to "/signup_users" do
        %button 
          登録する
  
  .logins__row__footer
    %p © Furima, Inc. All Rights Reserved.     
